end   

fe = zeros(8, 1);
for k = 1:4
    s = stress{k};
    
    Ak = Amat(defgrad{k});
    Bk = B{k} + Ak*H{k};
    
    fe = fe + Bk'*s*dJ{k}*t;
end
end